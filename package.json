{
  "name": "env-templater",
  "version": "0.0.0",
  "description": "Creates env files from templates with SSM support for various api clients.",
  "keywords": [
    "env templater",
    "environment files",
    "typescript",
    "bruno"
  ],
  "homepage": "https://github.com/dkhunt27/env-templater",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "env-templater": "src/bin/env-templater"
  },
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "cli": "npm run build && node dist/index.js",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "biome check --error-on-warnings",
    "format": "biome format --write",
    "test": "vitest run --coverage"
  },
  "author": "dkhunt27@gmail.com",
  "repository": "github:dkhunt27/env-templater",
  "bugs": {
    "url": "https://github.com/dkhunt27/env-templater/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@nestjs/testing": "^11.1.7",
    "@suites/di.nestjs": "^3.0.1",
    "@suites/doubles.vitest": "^3.0.1",
    "@suites/unit": "^3.0.1",
    "@types/node": "^22.18.1",
    "@vitest/coverage-istanbul": "3.2.4",
    "tsdown": "^0.15.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.8",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.916.0",
    "class-transformer": "^0.5.1",
    "fast-glob": "^3.3.3",
    "nest-commander": "^3.18.0"
  }
}
